#include <iostream>
using namespace std;

int main() {
    int n = 0;
    cin >> n;

    int ten = n / 10;
    cout << "NT10=" << ten << endl;

    int five = (n - ten * 10) / 5;
    cout << "NT5=" << five << endl;

    int one = n - ten * 10 - five * 5;
    cout << "NT1=" << one << endl;

    return 0;
}
